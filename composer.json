{
    "name": "snipershady/typeidentifier",
    "description": "Free service to identify primitive type of a variable",
    "keywords": ["strict type", "type", "primitive"],
    "homepage": "https://www.spinfo.it",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Stefano Perrini",
            "email": "perrini.stefano@gmail.com",
            "homepage": "https://www.spinfo.it"
        }
    ],
    "autoload": {
        "psr-4": {
            "TypeIdentifier\\": "src/"
        }       
    },
    "autoload-dev": {
        "psr-4": {
            "TypeIdentifier\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "phpstan/phpstan": "*"
    }
}
